package sec1;

public class type2 {

	public static void main(String[] args) {
		//reference(참조형) type: 여러개(다수)의 데이터를 저장하기 위한 기억장소로서 null 값이 허용됨
		int[] arr1 = {10,20,30};
		int[] arr2 = {10,20,30};
		// int[] arr3 = null; 
		int[] arr3;
		//reference type는 null값을 허용하나 pointer(히픠영역의 주소)가 null 이 되어 참조할수 없는 예외가 발생됨
		//int a = null; - primitive type 는 null 값 일수 없다.
		
		System.out.println(arr1==arr2);
		System.out.println(arr1!=arr2);
		//참조형은 간접비교를 해야 하므로 직접비교하게 되면, 포인터주소를 비교하게 되므로 서로 다르게 인식된다. 
	}

}
